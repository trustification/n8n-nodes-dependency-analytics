{
  "name": "@trustify-da/n8n-nodes-dependency-analytics",
  "version": "1.0.1",
  "description": "Trustify :: Dependency Analytics :: n8n",
  "keywords": [
    "n8n-community-node-package",
    "trustify",
    "n8n",
    "red-hat",
    "software-supply-chain",
    "security",
    "vulnerability"
  ],
  "license": "MIT",
  "homepage": "https://github.com/trustification/n8n-nodes-dependency-analytics",
  "author": {
    "name": "Trustify Dependency Analytics Authors",
    "email": "aoren@redhat.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trustification/n8n-nodes-dependency-analytics.git"
  },
  "engines": {
    "node": ">=22.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "test": "jest",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint --fix .",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "!*",
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RHTPAClientCredsOAuth2Api.credentials.js",
      "dist/credentials/TrustifyClientCredsOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DependencyAnalytics/DependencyAnalytics.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.2",
    "ts-jest": "^29.2.5",
    "jest": "^30.2.0",
    "@types/jest": "^30.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
